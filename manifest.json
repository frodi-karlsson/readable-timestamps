{
    "name": "Readable timestamps",
    "description": "Replaces timestamps in a page with date strings",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "icons": {
      "16": "images/icon-16.jpeg",
      "32": "images/icon-32.jpeg",
      "48": "images/icon-48.jpeg",
      "128": "images/icon-128.jpeg"
    },
    "action": {
      "default_icon": {
        "16": "images/icon-32.jpeg"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }